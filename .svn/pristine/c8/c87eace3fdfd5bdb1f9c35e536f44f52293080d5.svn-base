public class PianQuArrearage
{
    public String org_name;//片区名称
    public String all_total;//欠费总额
    public String s_total;//经营用水
    public String m_total;//居民生活
    public String q_total;//非经营用水

    public String getOrg_name()
    {
        return org_name == null ? "" : org_name;
    }

    public String getAll_total()
    {
        return all_total == null ? "0" : all_total;
    }

    public String getS_total()
    {
        return s_total == null ? "0" : s_total;
    }

    public String getM_total()
    {
        return m_total == null ? "0" : m_total;
    }

    public String getQ_total()
    {
        return q_total == null ? "0" : q_total;
    }
}
